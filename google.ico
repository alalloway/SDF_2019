                            <script>


                                function desktopHeaderToggleDropdown(event, id) {
                                    event.preventDefault();
                                    if ($acn(id + 'HeaderItem').is('.activeCategory')) {
                                        closeDropdown(id);
                                    }
                                    else {
                                        if ($acn('.activeCategory').size() > 0) {
                                            $acn('.linksDropDown').hide();
                                            $acn('.activeCategory').removeClass('activeCategory');
                                            $acn(id).show();
                                        } else {
                                            $acn(id).slideDown();
                                        }
                                        $acn(id + 'HeaderItem').toggleClass('activeCategory');
                                    }
                                    return false;
                                }

                                function toggleDropdown(event, id) {
                                    event.preventDefault();
                                    // if the selected top level menu dropdown is already expanded, collapse it
                                    if ($acn(id + 'HeaderItem').is('.activeCategory')) {
                                        closeDropdown(id);
                                    } else {
                                        //if another menu is already expanded, collapse it first
                                        if ($acn('.activeCategory').size() > 0) {
                                            $acn('.linksDropDown').removeAttr('style').hide();
                                            $acn('.activeCategory').removeClass('activeCategory');
                                            // $(id).slideDown(400, menuHeight(id));
                                        } else {
                                            // $(id).slideDown(400, menuHeight(id));
                                            // $(id + 'HeaderItem').toggleClass('activeCategory');
                                        }
                                        $acn(id).slideDown(400, menuHeight(id));
                                        $acn(id + 'HeaderItem').addClass('activeCategory');
                                    }
                                    return false;
                                }

                                function menuParentControl(e) {

                                }

                                function menuHeight(menu) {
                                    //  $('.topnavigation.topnavdropdown').css('height', $(menu).height());
                                }


                                function closeDropdown(id) {
                                    $acn('.topnavigation.topnavdropdown').removeAttr('style');
                                    $acn(id).slideUp();
                                    $acn(id + 'HeaderItem').removeClass('activeCategory');

                                }


                                function showTab(element) {
                                    if (!$acn(element).hasClass('active')) {
                                        $acn(element).tab('show');
                                    }
                                    $acn('.subCategoryButton.active,.subCategoryPane.active').removeClass('active');
                                }

                                function makeLinksAccessible() {
                                    $acn('a.accessibleLink').keypress(function (e) {
                                        if (e.which === 13) {
                                            $acn(e.target).click();
                                        }
                                    });
                                }

                                $acn(function () {
                                    makeLinksAccessible();
                                    $acn(".linksDropDown").hover(function () {
                                        $acn(this).addClass('mousedIn');
                                    }, function () {
                                        var $this = $acn(this);
                                        $this.removeClass('mousedIn');
                                        var slideUp = setInterval(function () {
                                            if (!$this.hasClass('mousedIn')) {
                                                closeDropdown('#' + $this.attr('id'));
                                                $('.topnavigation.topnavdropdown').css('height');
                                                clearInterval(slideUp);

                                            }
                                        }, 2000);
                                    });

